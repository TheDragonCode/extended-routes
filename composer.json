{
    "name": "andrey-helldar/extended-resource-router",
    "description": "Expansion of the functional of the Laravel resources routes",
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/andrey-helldar/extended-resource-router",
    "keywords": ["extended router", "resource", "routes", "router", "laravel"],
    "support": {
        "issues": "https://github.com/andrey-helldar/extended-resource-router/issues",
        "source": "https://github.com/andrey-helldar/extended-resource-router"
    },
    "authors": [
        {
            "name": "Andrey Helldar",
            "email": "helldar@ai-rus.com"
        }
    ],
    "require": {
        "php": ">=5.6.9",
        "illuminate/routing": "5.4.*|5.5.*|5.6.*",
        "illuminate/support": "5.4.*|5.5.*|5.6.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "classmap": [
            "src/"
        ],
        "psr-4": {
            "Helldar\\ExtendedResourceRouter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Helldar\\ExtendedResourceRouter\\ServiceProvider"
            ]
        }
    }
}
